urxvt*background:               rgba:0000/0000/0200/c800
# urxvt*font:                     xft:hermit:pixelsize=14:antialias=true
# urxvt*font:                    xft:SourceCodePro:pixelsize=16:antialias=true
urxvt*font:                     xft:DroidSansMono:pixelsize=16:antialias=true
urxvt*foreground:               [100]#ffffff
urxvt*depth:                    32
urxvt*scrollBar:		false
urxvt*pointerBlank:		true
urxvt*scrollTtyOutput:		false
urxvt*scrollWithBuffer:		true
urxvt*scrollTtyKeypress:	true
urxvt*saveLines:		10000
urxvt*shading:                  40
urxvt*termName:			rxvt

URxvt.depth:                    32
# URxvt.url-launcher:             /usr/bin/google-chrome-stable

#Xft.dpi: 120
! hintnone, hintslight, hintmedium, hintfull
Xft.hintstyle: hintfull
Xft.hinting: 1
! lcdnone, lcddefault, lcdlight, lcdlegacy
Xft.lcdfilter: lcddefault
Xft.rgba: rgb

Xft.autohint: 0
Xft.antialias: 1

XTerm.vt100.faceName: DroidSansMono:size=16:antialias=true
