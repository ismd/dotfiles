layout {
    default_tab_template {
        pane size=1 borderless=true {
            plugin location="tab-bar"
        }
        children
        pane size=1 borderless=true {
            plugin location="status-bar"
        }
    }
    tab name="main" focus=true {
        pane
    }
    tab name="a" {
        pane cwd="~/a"
    }
    tab name="cloud-console" {
        pane cwd="~/a/data-ui/cloud-console"
    }
}
